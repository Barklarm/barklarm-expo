{
  "name": "barklarm",
  "version": "1.0.5",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:tunnel": "expo start --tunnel",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --passWithNoTests --runInBand",
    "test:cov": "jest --passWithNoTests --coverage --runInBand",
    "test:dev": "jest --watch --runInBand",
    "tsc:check": "tsc --noEmit --skipLibCheck",
    "lint:check": "eslint src --ext .ts,.tsx --max-warnings=0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format:fix": "yarn prettier --write src",
    "format:check": "yarn prettier --check src",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "commit": "git add . && cz",
    "eas": "eas",
    "release:patch": "yarn version --patch && git push",
    "release:minor": "yarn version --patch && git push"
  },
  "dependencies": {
    "@expo/webpack-config": "^0.17.0",
    "@octokit/rest": "^19.0.4",
    "@react-native-picker/picker": "2.4.2",
    "@react-navigation/bottom-tabs": "^6.3.2",
    "@react-navigation/material-bottom-tabs": "^6.2.2",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "expo": "~46.0.3",
    "expo-background-fetch": "~10.3.0",
    "expo-notifications": "~0.16.1",
    "expo-permissions": "~13.2.0",
    "expo-secure-store": "~11.3.0",
    "expo-status-bar": "~1.4.0",
    "expo-task-manager": "~10.3.0",
    "expo-updates": "~0.14.4",
    "fast-xml-parser": "^4.0.9",
    "jest-expo": "^46.0.1",
    "octokit": "^2.0.7",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.4",
    "react-native-paper": "5.0.0-rc.4",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-svg": "12.3.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "~0.18.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-typescript": "^7.18.6",
    "@faker-js/faker": "^7.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.7",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.69.1",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "commitizen": "4.2.5",
    "eas-cli": "^0.60.0",
    "eslint": "8.22.0",
    "eslint-plugin-import": "2.26.0",
    "husky": "8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "ts-jest": "^28.0.8",
    "typescript": "~4.5.4"
  },
  "private": true
}
